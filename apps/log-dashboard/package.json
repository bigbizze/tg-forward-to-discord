{
  "name": "@tg-discord/log-dashboard",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsgo -b",
    "typecheck": "tsgo --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:logs": "RUN_LOG_DASHBOARD_IN_DEVELOPMENT=\"true\" pnpx nodemon --ext ts --watch src --watch ../../packages --exec \"pnpx tsx\" src/index.ts",
    "get-logs": "pnpx pm2 logs \"/(discord-bot|python-scrape|express-server)\/\" --lines 100 ",
    "get-logs:old": "tail -f ~/.pm2/logs/*out.log"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "express": "^4.18.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  }
}
