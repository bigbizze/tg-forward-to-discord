{
  "name": "@tg-discord/express-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsgo -b",
    "typecheck": "tsgo --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:run": "pnpx nodemon --ext ts --watch src --watch ../../packages --exec \"pnpx tsx\" src/index.ts",
    "dev:watch": "tsgo -b --watch --preserveWatchOutput",
    "dev": "pnpx concurrently \"pnpm dev:run\" \"pnpm dev:watch\"",
    "clean": "pnpx rimraf dist tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@tg-discord/config": "workspace:*",
    "@tg-discord/db": "workspace:*",
    "@tg-discord/discord-webhook": "workspace:*",
    "@tg-discord/result": "workspace:*",
    "@tg-discord/shared-types": "workspace:*",
    "async-retry": "^1.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/express": "^4.17.0"
  }
}
